{	"homepage"	:	"https://github.com/apache/maven-mvnd/releases/"	,	"license"	:	"Apache License"	,	"source"	:	{	"official"	:	[	"https://github.com/apache/maven-mvnd/releases"	]	}	,	"_os"	:	"%{os}"	,	"_arch"	:	"%{arch}"	,	"download_file_ext"	:	"zip"	,	"url"	:	{	"internet"	:	"https://github.com/apache/maven-mvnd/releases/download/%{semantic_version}/maven-mvnd-%{semantic_version}-m39-%{_os}-%{_arch}.%{download_file_ext}"	,	"cn"	:	"https://github.com/apache/maven-mvnd/releases/download/%{semantic_version}/maven-mvnd-%{semantic_version}-m39-%{_os}-%{_arch}.%{download_file_ext}"	}	,	"unpack"	:	"folder"	,	"sociality"	:	"adaptive"	,	"dep"	:	[	"java=v18.0.2-sem"	]	,	"hook"	:	{	"populate"	:	"populate.sh"	,	"boot"	:	"boot.sh"	,	"unboot"	:	"unboot.sh"	}	,	"xbin"	:	{	"mvnd"	:	"shim_bin/mvnd"	}	,	"path"	:	{	"bin"	:	[	"adaptive_shim_bin"	,	"mvn/bin"	]	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v1.0-m6"	}	,	"latest/*"	:	{	"version"	:	"v1.0-m6"	}	,	"*/linux/x64"	:	{	"_arch"	:	"amd64"	}	,	"*/darwin/x64"	:	{	"_arch"	:	"amd64"	}	,	"*/win/x64"	:	{	"_os"	:	"windows"	,	"_arch"	:	"amd64"	}	}	}	