{	"homepage"	:	"https://golang.google.cn/dl/"	,	"license"	:	"MIT"	,	"source"	:	{	"official"	:	[	"https://go.dev/dl"	,	"https://golang.google.cn/dl"	]	}	,	"go_url"	:	"https://go.dev/dl"	,	"golang_url"	:	"https://golang.google.cn/dl"	,	"_os"	:	"%{os}"	,	"_arch"	:	"%{arch}"	,	"download_file_ext"	:	"tar.gz"	,	"url"	:	{	"internet"	:	"%{go_url}/go%{semantic_version}.%{_os}-%{_arch}.%{download_file_ext}"	,	"cn"	:	"%{golang_url}/go%{semantic_version}.%{_os}-%{_arch}.%{download_file_ext}"	}	,	"unpack"	:	"folder"	,	"hook"	:	{	"populate"	:	"populate.sh"	,	"boot"	:	"boot.sh"	,	"unboot"	:	"unboot.sh"	}	,	"xbin"	:	{	"go"	:	"shim_bin/go%{bin_suffix}"	,	"gofmt"	:	"shim_bin/gofmt%{bin_suffix}"	}	,	"path"	:	{	"bin"	:	"shim_bin"	,	"lib"	:	"lib"	}	,	"sociality"	:	"intrusive"	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v1.22.5"	}	,	"latest/*"	:	{	"version"	:	"v1.22.5"	}	,	"*/linux/x64"	:	{	"_arch"	:	"amd64"	}	,	"*/linux/arm64"	:	null	,	"*/darwin/x64"	:	{	"_arch"	:	"amd64"	}	,	"*/darwin/arm64"	:	null	,	"*/win/x64"	:	{	"_os"	:	"windows"	,	"_arch"	:	"amd64"	,	"download_file_ext"	:	"zip"	,	"bin_suffix"	:	".exe"	}	}	}	