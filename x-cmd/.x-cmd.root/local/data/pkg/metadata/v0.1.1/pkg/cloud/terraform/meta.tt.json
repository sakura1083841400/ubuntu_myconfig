{	"homepage"	:	"https://github.com/hashicorp/terraform"	,	"license"	:	"MIT"	,	"source"	:	{	"x-cmd"	:	[	"https://github.com/x-cmd-build/terraform/releases"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/terraform/releases/download/%{version}"	,	"download_file_ext"	:	"tar.xz"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"zipname"	:	"terraform_%{os}_%{arch}.tar.xz"	,	"unpack"	:	"folder"	,	"xbin"	:	{	"terraform"	:	"bin/terraform%{bin_suffix}"	}	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v0.0.1"	}	,	"latest/*"	:	{	"version"	:	"v0.0.1"	}	,	"*/linux/arm64"	:	null	,	"*/darwin/arm64"	:	null	,	"*/linux/x64"	:	null	,	"*/darwin/x64"	:	null	,	"*/win/x64"	:	{	"bin_suffix"	:	".exe"	}	}	}	