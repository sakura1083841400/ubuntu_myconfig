{	"homepage"	:	"https://pandoc.org/"	,	"license"	:	"MIT"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/pandoc/releases"	,	"https://npmjs.com/package/@x-cmd-pkg/pandoc"	,	"https://npmmirror.com/package/@x-cmd-pkg/pandoc"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/pandoc/releases/download/%{semantic_version}"	,	"download_file_ext"	:	"tar.xz"	,	"zipname"	:	"%{tree}.%{download_file_ext}"	,	"unpack"	:	"folder"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"xbin"	:	{	"pandoc"	:	"bin/pandoc%{bin_suffix}"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v3.1.11"	}	,	"latest/*"	:	{	"version"	:	"v3.1.11"	}	,	"*/linux/arm64"	:	null	,	"*/darwin/arm64"	:	null	,	"*/linux/x64"	:	null	,	"*/darwin/x64"	:	null	,	"*/win/x64"	:	null	,	"v0.0.1/*/*"	:	{	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gc_url}/%{zipname}"	}	,	"gh_url"	:	"https://github.com/x-cmd-build/pandoc/releases/download/%{version}"	,	"gc_url"	:	"https://gitcode.net/x-cmd-build/pandoc/-/releases/%{version}/downloads"	,	"zipname"	:	"pandoc_%{os}_%{arch}.tar.xz"	}	,	"v3.1.11/*/*"	:	{	"npm"	:	"yes"	,	"npmurl"	:	{	"internet"	:	"https://registry.npmjs.org/@x-cmd-pkg/pandoc/-/pandoc-0.0.0-%{version}-%{tree}.tgz"	,	"cn"	:	"https://registry.npmmirror.com/@x-cmd-pkg/pandoc/-/pandoc-0.0.0-%{version}-%{tree}.tgz"	}	}	}	}	