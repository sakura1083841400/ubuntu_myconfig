{	"homepage"	:	"https://github.com/mirror/busybox"	,	"license"	:	"MIT"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/busybox/releases"	,	"https://npmjs.com/package/@x-cmd-pkg/busybox"	,	"https://npmmirror.com/package/@x-cmd-pkg/busybox"	]	}	,	"_os"	:	"%{os}"	,	"_arch"	:	"%{arch}"	,	"gh_url"	:	"https://github.com/x-cmd-build/busybox/releases/download/%{version}"	,	"download_file_ext"	:	"tar.xz"	,	"url"	:	{	"internet"	:	"%{gh_url}/busybox_%{_os}_%{_arch}.%{download_file_ext}"	,	"cn"	:	"%{gh_url}/busybox_%{_os}_%{_arch}.%{download_file_ext}"	}	,	"npm"	:	"yes"	,	"npmurl"	:	{	"internet"	:	"https://registry.npmjs.org/@x-cmd-pkg/busybox/-/busybox-0.0.0-%{version}-%{tree}.tgz"	,	"cn"	:	"https://registry.npmmirror.com/@x-cmd-pkg/busybox/-/busybox-0.0.0-%{version}-%{tree}.tgz"	}	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"xbin"	:	{	"busybox"	:	"bin/busybox%{bin_suffix}"	}	,	"unpack"	:	"folder"	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v0.0.1"	}	,	"latest/*"	:	{	"version"	:	"v0.0.1"	}	,	"*/linux/x64"	:	null	,	"*/linux/arm64"	:	null	,	"*/win/x64"	:	{	"bin_suffix"	:	".exe"	}	}	}	