{	"homepage"	:	"https://github.com/Canop/broot"	,	"license"	:	"MIT"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/broot/releases"	,	"https://npmjs.com/package/@x-cmd-pkg/broot"	,	"https://npmmirror.com/package/@x-cmd-pkg/broot"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/broot/releases/download/%{semantic_version}"	,	"download_file_ext"	:	"tar.xz"	,	"zipname"	:	"%{tree}.%{download_file_ext}"	,	"unpack"	:	"folder"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"npm"	:	"yes"	,	"npmurl"	:	{	"internet"	:	"https://registry.npmjs.org/@x-cmd-pkg/broot/-/broot-0.0.0-%{version}-%{tree}.tgz"	,	"cn"	:	"https://registry.npmmirror.com/@x-cmd-pkg/broot/-/broot-0.0.0-%{version}-%{tree}.tgz"	}	,	"xbin"	:	{	"broot"	:	"bin/broot%{bin_suffix}"	}	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v1.39.0"	}	,	"latest/*"	:	{	"version"	:	"v1.39.0"	}	,	"*/linux/x64"	:	{	"_build"	:	"glib"	}	,	"*/linux/arm64"	:	{	"_build"	:	"glib"	}	,	"*/win/x64"	:	{	"bin_suffix"	:	".exe"	}	,	"v1.23.0/*/*"	:	{	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gc_url}/%{zipname}"	}	,	"gh_url"	:	"https://github.com/x-cmd-build/broot/releases/download/%{version}"	,	"gc_url"	:	"https://gitcode.net/x-cmd-build/broot/-/releases/%{version}/downloads"	,	"zipname"	:	"broot_%{os}_%{arch}.%{download_file_ext}"	}	}	}	