{	"homepage"	:	"https://github.com/chriswalz/bit"	,	"license"	:	"MIT"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/bit/releases"	,	"https://npmjs.com/package/@x-cmd-pkg/bit"	,	"https://npmmirror.com/package/@x-cmd-pkg/bit"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/bit/releases/download/%{semantic_version}"	,	"download_file_ext"	:	"tar.xz"	,	"zipname"	:	"%{tree}.%{download_file_ext}"	,	"unpack"	:	"folder"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"xbin"	:	{	"bit"	:	"bin/bit%{bin_suffix}"	}	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v1.1.2"	}	,	"latest/*"	:	{	"version"	:	"v1.1.2"	}	,	"*/linux/arm64"	:	null	,	"*/darwin/arm64"	:	null	,	"*/linux/x64"	:	null	,	"*/darwin/x64"	:	null	,	"*/win/x64"	:	{	"bin_suffix"	:	".exe"	}	,	"v0.0.1/*/*"	:	{	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gc_url}/%{zipname}"	}	,	"gh_url"	:	"https://github.com/x-cmd-build/bit/releases/download/%{version}"	,	"gc_url"	:	"https://gitcode.net/x-cmd-build/bit/-/releases/%{version}/downloads"	,	"zipname"	:	"bit_%{os}_%{arch}.tar.xz"	}	,	"v1.1.2/*/*"	:	{	"npm"	:	"yes"	,	"npmurl"	:	{	"internet"	:	"https://registry.npmjs.org/@x-cmd-pkg/bit/-/bit-0.0.0-%{version}-%{tree}.tgz"	,	"cn"	:	"https://registry.npmmirror.com/@x-cmd-pkg/bit/-/bit-0.0.0-%{version}-%{tree}.tgz"	}	}	}	}	