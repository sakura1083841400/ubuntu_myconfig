{	"homepage"	:	"https://github.com/containers/bubblewrap"	,	"license"	:	"MIT"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/bwrap/releases"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/bwrap/releases/download/%{version}"	,	"download_file_ext"	:	"tar.xz"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"zipname"	:	"bwrap_%{os}_%{arch}.tar.xz"	,	"xbin"	:	{	"bwrap"	:	"bin/bwrap%{bin_suffix}"	}	,	"unpack"	:	"folder"	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v0.8.0"	}	,	"latest/*"	:	{	"version"	:	"v0.8.0"	}	,	"*/linux/arm64"	:	null	,	"*/linux/x64"	:	null	}	}	