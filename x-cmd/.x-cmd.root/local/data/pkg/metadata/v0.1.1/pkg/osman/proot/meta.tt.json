{	"homepage"	:	"https://github.com/proot-me/proot-rs"	,	"license"	:	"MIT"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/proot/releases"	,	"https://npmjs.com/package/@x-cmd-pkg/proot"	,	"https://npmmirror.com/package/@x-cmd-pkg/proot"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/proot/releases/download/%{semantic_version}"	,	"download_file_ext"	:	"tar.xz"	,	"zipname"	:	"%{tree}.%{download_file_ext}"	,	"unpack"	:	"folder"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"xbin"	:	{	"proot"	:	"bin/proot-rs"	}	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v0.1.0"	}	,	"latest/*"	:	{	"version"	:	"v0.1.0"	}	,	"*/linux/x64"	:	{	"_build"	:	"glib"	}	,	"*/linux/arm64"	:	{	"_build"	:	"glib"	}	,	"v0.0.1/*/*"	:	{	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gc_url}/%{zipname}"	}	,	"gh_url"	:	"https://github.com/x-cmd-build/proot/releases/download/%{version}"	,	"gc_url"	:	"https://gitcode.net/x-cmd-build/proot/-/releases/%{version}/downloads"	,	"zipname"	:	"proot_%{os}_%{arch}.tar.xz"	}	,	"v0.1.0/*/*"	:	{	"npm"	:	"yes"	,	"npmurl"	:	{	"internet"	:	"https://registry.npmjs.org/@x-cmd-pkg/proot/-/proot-0.0.0-%{version}-%{tree}.tgz"	,	"cn"	:	"https://registry.npmmirror.com/@x-cmd-pkg/proot/-/proot-0.0.0-%{version}-%{tree}.tgz"	}	}	}	}	