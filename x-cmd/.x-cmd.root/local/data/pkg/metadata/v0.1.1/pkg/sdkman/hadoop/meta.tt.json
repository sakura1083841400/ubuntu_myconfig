{	"homepage"	:	"https://groovy.jfrog.io/ui/native/dist-release-local/groovy-zips/"	,	"license"	:	"Apache License"	,	"source"	:	{	"repack-thirdparty"	:	[	"https://dlcdn.apache.org/hadoop"	]	}	,	"_os"	:	"%{os}"	,	"_arch"	:	"%{arch}"	,	"download_file_ext"	:	"tar.gz"	,	"url"	:	{	"internet"	:	"https://dlcdn.apache.org/hadoop/common/hadoop-%{semantic_version}/hadoop-%{semantic_version}.tar.gz"	,	"cn"	:	"https://dlcdn.apache.org/hadoop/common/hadoop-%{semantic_version}/hadoop-%{semantic_version}.tar.gz"	}	,	"unpack"	:	"folder"	,	"dep"	:	[	"java=v18.0.2-sem"	]	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"xbin"	:	{	"hadoop"	:	"shim_bin/hadoop"	}	,	"path"	:	{	"bin"	:	[	"shim_bin"	,	"share/hadoop/tools/sls/bin"	]	,	"lib"	:	"lib"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v3.3.5"	}	,	"latest/*"	:	{	"version"	:	"v3.3.5"	}	,	"*/linux/x64"	:	null	,	"*/darwin/x64"	:	null	,	"*/linux/arm64"	:	null	,	"v3.3.5/linux/arm64"	:	{	"url"	:	{	"internet"	:	"https://dlcdn.apache.org/hadoop/common/hadoop-%{semantic_version}/hadoop-%{semantic_version}-aarch64.tar.gz"	,	"cn"	:	"https://dlcdn.apache.org/hadoop/common/hadoop-%{semantic_version}/hadoop-%{semantic_version}-aarch64.tar.gz"	}	}	,	"v3.3.5/darwin/arm64"	:	{	"url"	:	{	"internet"	:	"https://dlcdn.apache.org/hadoop/common/hadoop-%{semantic_version}/hadoop-%{semantic_version}-aarch64.tar.gz"	,	"cn"	:	"https://dlcdn.apache.org/hadoop/common/hadoop-%{semantic_version}/hadoop-%{semantic_version}-aarch64.tar.gz"	}	}	}	}	