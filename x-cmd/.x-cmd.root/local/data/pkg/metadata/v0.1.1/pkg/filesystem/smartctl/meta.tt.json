{	"homepage"	:	"https://github.com/smartmontools/smartmontools"	,	"license"	:	"MIT"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/smartmontools/releases"	,	"https://npmjs.com/package/@x-cmd-pkg/smartctl"	,	"https://npmmirror.com/package/@x-cmd-pkg/smartctl"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/smartmontools/releases/download/%{version}"	,	"download_file_ext"	:	"tar.xz"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"npm"	:	"yes"	,	"npmurl"	:	{	"internet"	:	"https://registry.npmjs.org/@x-cmd-pkg/smartctl/-/smartctl-0.0.0-%{version}-%{tree}.tgz"	,	"cn"	:	"https://registry.npmmirror.com/@x-cmd-pkg/smartctl/-/smartctl-0.0.0-%{version}-%{tree}.tgz"	}	,	"zipname"	:	"smartctl_%{os}_%{arch}.tar.xz"	,	"xbin"	:	{	"smartctl"	:	"bin/smartctl%{bin_suffix}"	}	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"unpack"	:	"folder"	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v0.0.1"	}	,	"latest/*"	:	{	"version"	:	"v0.0.1"	}	,	"*/darwin/arm64"	:	null	,	"*/linux/x64"	:	null	,	"*/darwin/x64"	:	null	,	"*/win/x64"	:	{	"bin_suffix"	:	".exe"	}	}	}	