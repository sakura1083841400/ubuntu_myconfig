{	"homepage"	:	"https://gradle.org/"	,	"license"	:	"Apache License"	,	"source"	:	{	"repack-thirdparty"	:	[	"https://services.gradle.org/distributions"	]	}	,	"_os"	:	"%{os}"	,	"_arch"	:	"%{arch}"	,	"download_file_ext"	:	"zip"	,	"url"	:	{	"internet"	:	"https://services.gradle.org/distributions/gradle-%{semantic_version}-bin.zip"	,	"cn"	:	"https://services.gradle.org/distributions/gradle-%{semantic_version}-bin.zip"	}	,	"unpack"	:	"folder"	,	"dep"	:	[	"java=v18.0.2-sem"	]	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"xbin"	:	{	"gradle"	:	"shim_bin/gradle"	}	,	"path"	:	{	"bin"	:	"shim_bin"	,	"lib"	:	"lib"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v8.1.1"	}	,	"latest/*"	:	{	"version"	:	"v8.1.1"	}	,	"*/linux/arm64"	:	null	,	"*/darwin/arm64"	:	null	,	"*/linux/x64"	:	null	,	"*/darwin/x64"	:	null	,	"*/win/x64"	:	null	}	}	