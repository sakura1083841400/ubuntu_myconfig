{	"homepage"	:	"https://github.com/fastly/cli"	,	"license"	:	"Apache-2.0"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/fastly/releases"	,	"https://npmjs.com/package/@x-cmd-pkg/fastly"	,	"https://npmmirror.com/package/@x-cmd-pkg/fastly"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/fastly/releases/download/%{semantic_version}"	,	"download_file_ext"	:	"tar.xz"	,	"zipname"	:	"%{tree}.%{download_file_ext}"	,	"unpack"	:	"folder"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"npm"	:	"yes"	,	"npmurl"	:	{	"internet"	:	"https://registry.npmjs.org/@x-cmd-pkg/fastly/-/fastly-0.0.0-%{version}-%{tree}.tgz"	,	"cn"	:	"https://registry.npmmirror.com/@x-cmd-pkg/fastly/-/fastly-0.0.0-%{version}-%{tree}.tgz"	}	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v10.8.0"	}	,	"latest/*"	:	{	"version"	:	"v10.8.0"	}	,	"*/linux/x64"	:	null	,	"*/linux/arm64"	:	null	,	"*/darwin/x64"	:	null	,	"*/darwin/arm64"	:	null	,	"*/win/x64"	:	{	"bin_suffix"	:	".exe"	}	}	}	