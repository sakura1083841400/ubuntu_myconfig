{	"homepage"	:	"https://github.com/charmbracelet/soft-serve"	,	"license"	:	"MIT"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/soft-serve/releases"	,	"https://npmjs.com/package/@x-cmd-pkg/soft-serve"	,	"https://npmmirror.com/package/@x-cmd-pkg/soft-serve"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/soft-serve/releases/download/%{semantic_version}"	,	"download_file_ext"	:	"tar.xz"	,	"zipname"	:	"%{tree}.%{download_file_ext}"	,	"unpack"	:	"folder"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"xbin"	:	{	"soft"	:	"bin/soft-serve%{bin_suffix}"	}	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v0.7.4"	}	,	"latest/*"	:	{	"version"	:	"v0.7.4"	}	,	"*/linux/arm64"	:	null	,	"*/darwin/arm64"	:	null	,	"*/linux/x64"	:	null	,	"*/darwin/x64"	:	null	,	"*/win/x64"	:	{	"bin_suffix"	:	".exe"	}	,	"v0.5.4/*/*"	:	{	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gc_url}/%{zipname}"	}	,	"gh_url"	:	"https://github.com/x-cmd-build/soft-serve/releases/download/%{version}"	,	"gc_url"	:	"https://gitcode.net/x-cmd-build/soft-serve/-/releases/%{version}/downloads"	,	"zipname"	:	"soft-serve_%{os}_%{arch}.%{download_file_ext}"	}	,	"v0.7.4/*/*"	:	{	"npm"	:	"yes"	,	"npmurl"	:	{	"internet"	:	"https://registry.npmjs.org/@x-cmd-pkg/soft-serve/-/soft-serve-0.0.0-%{version}-%{tree}.tgz"	,	"cn"	:	"https://registry.npmmirror.com/@x-cmd-pkg/soft-serve/-/soft-serve-0.0.0-%{version}-%{tree}.tgz"	}	}	}	}	