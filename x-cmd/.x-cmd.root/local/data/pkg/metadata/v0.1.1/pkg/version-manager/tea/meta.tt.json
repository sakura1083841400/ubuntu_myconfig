{	"homepage"	:	"https://gitea.com/gitea/tea"	,	"license"	:	"MIT"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/tea/releases"	,	"https://npmjs.com/package/@x-cmd-pkg/tea"	,	"https://npmmirror.com/package/@x-cmd-pkg/tea"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/tea/releases/download/%{semantic_version}"	,	"install-text"	:	{	"x"	:	false	,	"cn"	:	"`x tea` 是由 x-cmd 团队基于 Gitea API 封装的 Gitea CLI 模块"	,	"en"	:	"`x tea` is a Gitea CLI module packaged by the x-cmd team based on the Gitea API."	}	,	"download_file_ext"	:	"tar.xz"	,	"zipname"	:	"%{tree}.%{download_file_ext}"	,	"unpack"	:	"folder"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"npm"	:	"yes"	,	"npmurl"	:	{	"internet"	:	"https://registry.npmjs.org/@x-cmd-pkg/tea/-/tea-0.0.0-%{version}-%{tree}.tgz"	,	"cn"	:	"https://registry.npmmirror.com/@x-cmd-pkg/tea/-/tea-0.0.0-%{version}-%{tree}.tgz"	}	,	"xbin"	:	{	"tea"	:	"bin/tea%{bin_suffix}"	}	,	"path"	:	{	"bin"	:	"bin"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v0.9.2"	}	,	"latest/*"	:	{	"version"	:	"v0.9.2"	}	,	"*/linux/arm64"	:	null	,	"*/darwin/arm64"	:	null	,	"*/linux/x64"	:	null	,	"*/darwin/x64"	:	null	,	"*/win/x64"	:	{	"bin_suffix"	:	".exe"	}	}	}	