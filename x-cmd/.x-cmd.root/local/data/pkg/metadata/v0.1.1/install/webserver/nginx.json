{
  "lang": "c",
  "homepage": "https://nginx.org/",
  "desc": {
    "cn": "nginx 是一个 HTTP 和反向代理服务器、邮件代理服务器和通用 TCP/UDP 代理服务器，最初由 Igor Sysoev 编写",
    "en": "nginx is an HTTP and reverse proxy server, a mail proxy server, and a generic TCP/UDP proxy server, originally written by Igor Sysoev"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install nginx",
      "reference": "https://formulae.brew.sh/formula/nginx#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install nginx-extras",
      "reference": "https://command-not-found.com/nginx"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install nginx-extras",
      "reference": "https://command-not-found.com/nginx"
    },
    "alpine/apk": {
      "cmd": "sudo apk add nginx",
      "reference": "https://command-not-found.com/nginx"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S nginx",
      "reference": "https://command-not-found.com/nginx"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install nginx-extras",
      "reference": "https://command-not-found.com/nginx"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install nginx-extras",
      "reference": "https://command-not-found.com/nginx"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install nginx-extras",
      "reference": "https://command-not-found.com/nginx"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge --ask www-servers/nginx",
      "reference": "https://wiki.gentoo.org/wiki/Nginx"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install nginx",
      "reference": "https://en.opensuse.org/Nginx"
    }
  }
}
