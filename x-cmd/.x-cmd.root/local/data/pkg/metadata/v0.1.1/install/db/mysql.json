{
  "lang": "C 和 C++",
  "homepage": "https://mysql.com",
  "desc": {
    "cn": "开源关系数据库管理系统",
    "en": "Open source relational database management system"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use mysql",
      "reference": "https://github.com/iroddis/asdf-mysql"
    },
    "darwin/brew": {
      "cmd": "brew install mysql",
      "reference": "https://formulae.brew.sh/formula/mysql#default"
    },
    "debian/apt": {
      "cmd": "sudo apt install mysql-server",
      "reference": "https://www.devart.com/dbforge/mysql/install-mysql-on-debian/"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install mysql-server",
      "reference": "https://ubuntu.com/server/docs/databases-mysql"
    },
    "alpine/apk": {
      "cmd": "sudo apk add mysql mysql-client",
      "reference": "https://wiki.alpinelinux.org/wiki/MySQL"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S mysql",
      "reference": "https://www.geeksforgeeks.org/how-to-install-and-configure-mysql-on-arch-based-linux-distributionsmanjaro/"
    },
    "kali/apt": {
      "cmd": "sudo apt install mysql-community-server",
      "reference": "https://computingforgeeks.com/how-to-install-mysql-on-kali-linux/"
    },
    "centos/yum": {
      "cmd": "sudo yum install mysql-server",
      "reference": "https://www.digitalocean.com/community/tutorials/how-to-install-mysql-on-centos-7"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install mysql-community-server",
      "reference": "https://docs.fedoraproject.org/en-US/quick-docs/installing-mysql-mariadb/"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install mysql-server",
      "reference": "https://bytesofgigabytes.com/raspberrypi/how-to-install-mysql-database-on-raspberry-pi/"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge --ask dev-db/mysql",
      "reference": "https://wiki.gentoo.org/wiki/MySQL"
    }
  }
}
