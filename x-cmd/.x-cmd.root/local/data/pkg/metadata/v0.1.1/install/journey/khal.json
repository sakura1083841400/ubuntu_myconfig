{
  "lang": "Python",
  "homepage": "http://lostpackets.de/khal/",
  "desc": {
    "cn": "Khal 是基于标准的 CLI（控制台）日历程序，能够通过 vdirsyncer 与 CalDAV 服务器同步",
    "en": "Khal is a standards based CLI (console) calendar program, able to synchronize with CalDAV servers through vdirsyncer"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install khal",
      "reference": "https://lostpackets.de/khal/install.html"
    },
    "debian/apt": {
      "cmd": "sudo apt install khal",
      "reference": "https://github.com/pimutils/khal?tab=readme-ov-file#debianubuntu"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install khal",
      "reference": "https://github.com/pimutils/khal?tab=readme-ov-file#debianubuntu"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install khal",
      "reference": "https://github.com/pimutils/khal?tab=readme-ov-file#fedora"
    }
  }
}
