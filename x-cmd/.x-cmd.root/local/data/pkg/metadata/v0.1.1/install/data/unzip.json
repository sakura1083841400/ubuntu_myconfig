{
  "lang": "c",
  "homepage": "https://infozip.sourceforge.net/UnZip.html",
  "desc": {
    "cn": "UnZip 是压缩为.zip 格式(也称为“ zipfiles”)的档案的提取工具",
    "en": "UnZip is an extraction utility for archives compressed in .zip format (also called \"zipfiles\")"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install unzip",
      "reference": "https://formulae.brew.sh/formula/unzip#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install unzip",
      "reference": "https://command-not-found.com/unzip"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install unzip",
      "reference": "https://command-not-found.com/unzip"
    },
    "alpine/apk": {
      "cmd": "sudo apk add unzip",
      "reference": "https://command-not-found.com/unzip"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S unzip",
      "reference": "https://command-not-found.com/unzip"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install unzip",
      "reference": "https://command-not-found.com/unzip"
    },
    "centos/yum": {
      "cmd": "sudo yum install unzip",
      "reference": "https://command-not-found.com/unzip"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install unzip",
      "reference": "https://command-not-found.com/unzip"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install unzip",
      "reference": "https://command-not-found.com/unzip"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge --ask app-arch/unzip",
      "reference": "https://wiki.gentoo.org/wiki/Unzip"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install unzip",
      "reference": "https://www.edustorage.net/entry/how-to-unzip-file-in-opensuse-linux"
    }
  }
}
