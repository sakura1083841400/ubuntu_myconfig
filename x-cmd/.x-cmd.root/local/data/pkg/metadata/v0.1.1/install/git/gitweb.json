{
  "lang": "rust",
  "homepage": "https://github.com/yoannfleurydev/gitweb",
  "desc": {
    "cn": "gitweb 是我创建的命令行接口，主要是为了学习 Rust",
    "en": "gitweb is a command line interface I created mainly to learn Rust"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install gitweb",
      "reference": "https://github.com/yoannfleurydev/gitweb?tab=readme-ov-file#-the-cargo-way"
    },
    "darwin/brew": {
      "cmd": "brew install yoannfleurydev/gitweb/gitweb",
      "reference": "https://github.com/yoannfleurydev/gitweb?tab=readme-ov-file#-the-homebrew-way"
    },
    "darwin/port": {
      "cmd": "sudo port install gitweb",
      "reference": "https://github.com/yoannfleurydev/gitweb?tab=readme-ov-file#-the-macports-way"
    }
  }
}
