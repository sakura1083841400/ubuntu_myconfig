{
  "lang": "rust",
  "homepage": "https://github.com/extrawurst/gitui",
  "desc": {
    "cn": "gitui 是一个基于终端的 Git 用户界面工具",
    "en": "gitui is a terminal-based Git user interface tool"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use gitui",
      "reference": "https://github.com/looztra/asdf-gitui"
    },
    "/cargo": {
      "cmd": "cargo install gitui",
      "reference": "https://github.com/extrawurst/gitui?tab=readme-ov-file#cargo-install"
    },
    "darwin/brew": {
      "cmd": "brew install gitui",
      "reference": "https://formulae.brew.sh/formula/gitui#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install gitui",
      "reference": "https://scoop.sh/#/apps?q=gitui"
    }
  }
}
