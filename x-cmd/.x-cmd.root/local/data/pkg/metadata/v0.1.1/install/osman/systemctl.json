{
  "lang": "c",
  "homepage": "https://systemd.io/",
  "desc": {
    "cn": "Systemd 是 Linux 系统的一套基本构建块",
    "en": "systemd is a suite of basic building blocks for a Linux system"
  },
  "rule": {
    "debian/apt": {
      "cmd": "sudo apt-get install systemd",
      "reference": "https://dockerfile.run/systemctl"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install systemd",
      "reference": "https://dockerfile.run/systemctl"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S systemd",
      "reference": "https://dockerfile.run/systemctl"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install systemd",
      "reference": "https://dockerfile.run/systemctl"
    },
    "centos/yum": {
      "cmd": "sudo yum install systemd",
      "reference": "https://dockerfile.run/systemctl"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install systemd",
      "reference": "https://dockerfile.run/systemctl"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install systemd",
      "reference": "https://dockerfile.run/systemctl"
    }
  }
}
