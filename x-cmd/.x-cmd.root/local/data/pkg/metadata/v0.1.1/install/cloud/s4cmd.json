{
  "lang": "python",
  "homepage": "https://github.com/bloomreach/s4cmd",
  "desc": {
    "cn": "超级 S3 命令行工具",
    "en": "Super S3 command line tool"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install s4cmd",
      "reference": "https://github.com/bloomreach/s4cmd#installation-and-setup"
    },
    "darwin/brew": {
      "cmd": "brew install s4cmd",
      "reference": "https://formulae.brew.sh/formula/s4cmd#default"
    },
    "debian/apt": {
      "cmd": "sudo apt install s4cmd",
      "reference": "https://command-not-found.com/s4cmd"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install s4cmd",
      "reference": "https://command-not-found.com/s4cmd"
    },
    "kali/apt": {
      "cmd": "sudo apt install s4cmd",
      "reference": "https://command-not-found.com/s4cmd"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install s4cmd",
      "reference": "https://command-not-found.com/s4cmd"
    }
  }
}
