{
  "homepage": "https://github.com/FFmpeg/FFmpeg",
  "desc": {
    "cn": "ffmpeg 是用于处理音频、视频、字幕和相关元数据等多媒体内容的库和工具的集合",
    "en": "ffmpeg is a collection of libraries and tools for working with multimedia content such as audio, video, subtitles, and related metadata"
  },
  "lang": "C",
  "rule": {
    "/asdf": {
      "cmd": "x asdf use ffmpeg",
      "reference": "https://github.com/acj/asdf-ffmpeg"
    },
    "darwin/brew": {
      "cmd": "brew install ffmpeg",
      "reference": "https://formulae.brew.sh/formula/ffmpeg#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install ffmpeg",
      "reference": "https://command-not-found.com/ffmpeg"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install ffmpeg",
      "reference": "https://command-not-found.com/ffmpeg"
    },
    "alpine/apk": {
      "cmd": "sudo apk add ffmpeg",
      "reference": "https://command-not-found.com/ffmpeg"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S ffmpeg",
      "reference": "https://command-not-found.com/ffmpeg"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install ffmpeg",
      "reference": "https://command-not-found.com/ffmpeg"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install ffmpeg",
      "reference": "https://command-not-found.com/ffmpeg"
    }
  }
}
