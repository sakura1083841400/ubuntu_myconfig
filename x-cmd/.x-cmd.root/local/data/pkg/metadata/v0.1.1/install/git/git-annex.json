{
  "lang": "Haskell",
  "homepage": "https://git-annex.branchable.com/",
  "desc": {
    "cn": "git-annex 是 git 用户命令行界面管理工具",
    "en": "git-annex is a command line interface management tool for git users"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install git-annex",
      "reference": "https://formulae.brew.sh/formula/git-annex#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install git-annex",
      "reference": "https://command-not-found.com/git-remote-tor-annex"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install git-annex",
      "reference": "https://command-not-found.com/git-remote-tor-annex"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S git-annex",
      "reference": "https://command-not-found.com/git-remote-tor-annex"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install git-annex",
      "reference": "https://command-not-found.com/git-remote-tor-annex"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install git-annex",
      "reference": "https://command-not-found.com/git-remote-tor-annex"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install git-annex",
      "reference": "https://command-not-found.com/git-remote-tor-annex"
    }
  }
}
