{
  "lang": "Go",
  "homepage": "https://github.com/jsonnet-bundler/jsonnet-bundler",
  "desc": {
    "cn": "jsonnet 软件包经理",
    "en": "A jsonnet package manager"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install jsonnet-bundler",
      "reference": "https://tanka.dev/install#jsonnet-bundler"
    },
    "arch/yay": {
      "cmd": "yay jsonnet-bundler-bin",
      "reference": "https://tanka.dev/install#jsonnet-bundler"
    },
    "linux/curl": {
      "cmd": "sudo curl -Lo /usr/local/bin/jb https://github.com/jsonnet-bundler/jsonnet-bundler/releases/latest/download/jb-linux-amd64 \\\n&& sudo chmod a+x /usr/local/bin/jb\n",
      "reference": "https://tanka.dev/install#jsonnet-bundler"
    },
    "darwin/curl": {
      "cmd": "sudo curl -Lo /usr/local/bin/jb https://github.com/jsonnet-bundler/jsonnet-bundler/releases/latest/download/jb-darwin-amd64 \\\n&& sudo chmod a+x /usr/local/bin/jb\n",
      "reference": "https://tanka.dev/install#jsonnet-bundler"
    },
    "/go": {
      "cmd": "go install github.com/jsonnet-bundler/jsonnet-bundler/cmd/jb@latest",
      "reference": "https://tanka.dev/install#jsonnet-bundler"
    }
  }
}
