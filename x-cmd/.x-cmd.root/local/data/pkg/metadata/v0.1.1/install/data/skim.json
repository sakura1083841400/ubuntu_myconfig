{
  "lang": "rust",
  "desc": {
    "cn": "Rust 中的模糊查找器",
    "en": "A fuzzy finder for Rust"
  },
  "homepage": "https://github.com/lotabout/skim",
  "rule": {
    "/cargo": {
      "cmd": "cargo install skim",
      "reference": "https://github.com/lotabout/skim?tab=readme-ov-file#hard-core"
    },
    "darwin/brew": {
      "cmd": "brew install sk",
      "reference": "https://github.com/lotabout/skim?tab=readme-ov-file#package-managers"
    },
    "darwin/port": {
      "cmd": "sudo port install skim",
      "reference": "https://github.com/lotabout/skim?tab=readme-ov-file#package-managers"
    },
    "fedora/dnf": {
      "cmd": "dnf install skim",
      "reference": "https://github.com/lotabout/skim?tab=readme-ov-file#package-managers"
    },
    "alpine/apk": {
      "cmd": "apk add skim",
      "reference": "https://github.com/lotabout/skim?tab=readme-ov-file#package-managers"
    },
    "arch/pacman": {
      "cmd": "pacman -S skim",
      "reference": "https://github.com/lotabout/skim?tab=readme-ov-file#package-managers"
    },
    "gentoo/emerge": {
      "cmd": "emerge --ask app-misc/skim",
      "reference": "https://github.com/lotabout/skim?tab=readme-ov-file#package-managers"
    }
  }
}
