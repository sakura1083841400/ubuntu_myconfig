{
  "lang": "python",
  "homepage": "http://mycli.net",
  "desc": {
    "cn": "MySQL、MariaDB 和 Percona 的命令行界面，具有自动完成和语法功能突出显示",
    "en": "MyCLI is a command line interface for MySQL, MariaDB, and Percona with auto-completion and syntax highlighting."
  },
  "rule": {
    "/pip": {
      "cmd": "pip install -U mycli",
      "reference": "https://github.com/dbcli/mycli#quick-start"
    },
    "/pip3": {
      "cmd": "pip3 install mycli",
      "reference": "https://www.mycli.net/install"
    },
    "darwin/brew": {
      "cmd": "brew update\n&& brew install mycli\n",
      "reference": "https://github.com/dbcli/mycli#quick-start"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install mycli",
      "reference": "https://github.com/dbcli/mycli#quick-start"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install mycli",
      "reference": "https://github.com/dbcli/mycli#quick-start"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install mycli",
      "reference": "https://command-not-found.com/mycli"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install mycli",
      "reference": "https://command-not-found.com/mycli"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install mycli",
      "reference": "https://command-not-found.com/mycli"
    }
  }
}
