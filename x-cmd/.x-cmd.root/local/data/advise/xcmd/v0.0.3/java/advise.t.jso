"#synopsis"
:
[
"javac <options> <source files>"
]
,
"@"
:
{
"#synopsis"
:
"<filename>"
,
"#desc"
:
{
"en"
:
"Read options and filenames from file"
,
"cn"
:
"从文件中读取选项和文件名"
}
,
"#1"
:
{
"#exec"
:
"___x_cmd_advise__file"
}
}
