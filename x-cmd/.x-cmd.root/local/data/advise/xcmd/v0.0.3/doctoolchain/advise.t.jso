"--no-build-cache"
:
{
"#desc"
:
{
"en"
:
"Disables the Gradle build cache"
,
"cn"
:
"禁用 Gradle 构建缓存"
}
}
,
"--no-configuration-cache"
:
{
"#desc"
:
{
"en"
:
"Disables the configuration cache."
,
"cn"
:
"禁用配置缓存。"
}
}
,
"--no-configure-on-demand"
:
{
"#desc"
:
{
"en"
:
"Disables the use of configuration on demand."
,
"cn"
:
"禁用按需配置。"
}
}
