:
{
"#cand"
:
[
"plain"
,
"auto"
,
"rich"
,
"verbose"
]
}
}
,
"--continue"
:
{
"#desc"
:
{
"en"
:
"Continue task execution after a task failure"
,
"cn"
:
"在任务失败后继续执行任务"
}
}
,
"-D|--system-prop"
:
{
"#synopsis"
:
"<string>"
,
"#desc"
:
{
"en"
:
"Set system property of the JVM (e.g. -Dmyprop=myvalue)"
,
"cn"
:
"设置 JVM 的系统属性（例如 -Dmyprop=myvalue）"
}
}
