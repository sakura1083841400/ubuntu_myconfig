:
null
,
"#desc"
:
{
"cn"
:
"GPU 卸载已禁用"
,
"en"
:
"GPU offloading is disabled"
}
}
,
{
"max"
:
null
,
"#desc"
:
{
"cn"
:
"所有层都卸载到 GPU"
,
"en"
:
"All layers are offloaded to GPU"
}
}
,
{
