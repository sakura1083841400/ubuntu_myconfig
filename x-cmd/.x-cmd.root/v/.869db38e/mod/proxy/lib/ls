# shellcheck shell=dash

___x_cmd_proxy_ls(){
    printf "Current proxy setting:\n"
    printf "  http_proxy=%s\n" "$http_proxy"
    printf "  HTTP_PROXY=%s\n" "$HTTP_PROXY"
    printf "  https_proxy=%s\n" "$https_proxy"
    printf "  HTTPS_PROXY=%s\n" "$HTTPS_PROXY"
    printf "  ftp_proxy=%s\n" "$ftp_proxy"
    printf "  FTP_PROXY=%s\n" "$FTP_PROXY"
    printf "  all_proxy=%s\n" "$all_proxy"
    printf "  ALL_PROXY=%s\n" "$ALL_PROXY"
}
