# shellcheck shell=dash

___x_cmd_qywx_jqu(){
    local IFS=" "
    printf "%s" "$*" | ___x_cmd_cmds_awk '
{
    text = (text == "") ? $0 : (text "\n" $0)
}
END{
    gsub(/\\/, "\\\\", text)
    gsub("\n", "\\n", text)
    gsub("\t", "\\t", text)
    gsub("\"", "\\\"", text)
    printf("\"%s\"", text)
}
'
}
