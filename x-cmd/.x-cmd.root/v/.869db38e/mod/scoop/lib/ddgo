# shellcheck shell=dash
___x_cmd_scoop_ddgo(){
    local X_help_cmd=; X_help_cmd='___x_cmd help -m scoop ddgo' help:arg:parse
    ___X_CMD_DDGO_QUERY_SITE="https://github.com/ScoopInstaller" \
        ___x_cmd ddgo "$@"
}

___x_cmd_scoop_ddgoai(){
    local X_help_cmd=; X_help_cmd='___x_cmd help -m scoop ddgoai' help:arg:parse
    ___X_CMD_DDGO_QUERY_SITE="https://github.com/ScoopInstaller" \
        ___x_cmd ddgo --ai "$@"
}

