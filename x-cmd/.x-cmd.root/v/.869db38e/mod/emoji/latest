xrc:mod:lib     emoji   main which  update
if ___x_cmd_is_suitable_advise_env; then
    xrc:mod:lib emoji    advise
fi

___x_cmd_emoji(){
    ___x_cmd_emoji___main "$@"
}

xrc setmain ___x_cmd_emoji
