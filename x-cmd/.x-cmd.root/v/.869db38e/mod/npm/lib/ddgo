# shellcheck shell=dash

___x_cmd_npm_ddgo(){
    case "$1" in
        -h|--help)          ___x_cmd_npm___help ddgo; return ;;
    esac

    ___X_CMD_DDGO_QUERY_SITE="https://www.npmjs.com" \
        ___x_cmd ddgo "$@"
}

___x_cmd_npm_ddgoai(){
    case "$1" in
        -h|--help)          ___x_cmd_npm___help ddgoai; return ;;
    esac

    ___X_CMD_DDGO_QUERY_SITE="https://www.npmjs.com" \
        ___x_cmd ddgo --ai "$@"
}
