x log init gx
xrc browse
xrc:mod:lib gx    main

___x_cmd_gx(){
    ___x_cmd_gx___main "$@"
}

alias ws="x gx ws"

x mkdirp "${___X_CMD_ROOT_TMP}/gx"
xrc setmain ___x_cmd_gx
