___x_cmd_mac_powermode_hibernate(){
    :
    # desktop
}


___x_cmd_mac_powermode_hibernate_set(){
    :
    # desktop mode

    local mode="desktop" # portable slow

    case "$mode" in
        desktop)        hibernatemode=0     ;;
        portable)       hibernatemode=3     ;;
        slow)           hibernatemode=25    ;;
    esac

}

