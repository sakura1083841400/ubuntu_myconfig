{
"#desc"
:
{
"en"
:
"Setting mirror source for development"
,
"cn"
:
"用于设置镜像源的工具"
}
,
"#tldr"
:
[
{
"cmd"
:
"x mirror brew set tuna"
,
"en"
:
"Set the homebrew mirror source to the Tsinghua University Open Source Software Mirror"
,
"cn"
:
"将 homebrew 的镜像源设置为清华大学开源软件镜像站"
}
,
{
"cmd"
:
"x mirror apt ls"
,
"en"
:
"List the available mirror source for apt"
,
"cn"
:
"列出 apt 的可用镜像源"
}
]
,
"#subcmd:lang"
:
[
"pip"
,
"npm"
,
"pnpm"
,
"go"
,
"gem"
,
"cargo"
,
"yarn"
]
,
"#subcmd:system"
:
[
"apt"
,
"brew"
,
"yum"
,
"apk"
,
"pacman"
,
"dnf"
]
,
"#subcmd:container"
:
[
"docker"
]
,
"pip"
:
{
"#desc"
:
{
"en"
:
"Setting the pip mirror for Python development"
,
"cn"
:
"设置 pip 的镜像源"
}
,
"ls"
:
{
"#desc"
:
{
"cn"
:
"列出当前可用的镜像源"
,
"en"
:
"List the available mirror source"
}
}
,
"set"
:
{
"#desc"
:
{
"en"
:
"Set the mirror source, select one from the current mirror source and then set it"
,
"cn"
:
"设置镜像源，从当前的镜像源中选择一个然后进行设置"
}
,
"#1"
:
{
"#desc"
:
{
"en"
:
"The mirror source name"
,
"cn"
:
"镜像源的名称"
}
,
"#exec"
:
"xrc mirror/lib/advise && ___x_cmd_mirror___advise_mirror_ls pip"
}
}
,
"current"
:
{
"#desc"
:
{
"en"
:
"Get the current mirror source url"
,
"cn"
:
"获取当前的镜像源的 url"
}
}
,
"unset"
:
{
"#desc"
:
{
"en"
:
"Reset the source to the original official"
,
"cn"
:
"将下载源设置为默认的源。"
}
}
}
