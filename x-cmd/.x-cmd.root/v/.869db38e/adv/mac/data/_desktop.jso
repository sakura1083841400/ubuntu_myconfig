"wallpaper"
:
{
"#desc"
:
{
"en"
:
"Desktop Wallpaper Management"
,
"cn"
:
"桌面壁纸管理"
}
,
"info"
:
{
"#desc"
:
{
"cn"
:
"获取指定桌面壁纸路径。(默认为1)"
,
"en"
:
"Get the wallpaper path of a specified desktop (Default is 1)."
}
,
"#1"
:
{
"cn"
:
"桌面编号"
,
"en"
:
"Desktop Number"
}
}
,
"set"
:
{
"#desc"
:
{
"cn"
:
"设置桌面壁纸为指定文件路径的图片。(需要在非全屏状态下设置)"
,
"en"
:
"Set the desktop wallpaper to an image from a specified file path (must be set when not in fullscreen mode)."
}
,
"#1"
:
{
"cn"
:
"图片路径"
,
"en"
:
"Image Path"
,
"#exec"
:
"___x_cmd_advise__file"
}
,
"#2"
:
{
"cn"
:
"桌面编号（默认为当前桌面）"
,
"en"
:
"Desktop Number (Defaulting to the current desktop)"
}
}
,
"#tldr"
:
[
{
"cmd"
:
"x mac wallpaper info 1"
,
"cn"
:
"查看桌面 1 使用的壁纸"
,
"en"
:
"Check out the wallpaper used on Desktop 1"
}
,
{
"cmd"
:
"x mac wallpaper set \"$HOME/xcmd.png\""
,
"cn"
:
"更换当前桌面的壁纸为 \"$HOME/xcmd.png\" 图片"
,
"en"
:
"Replace the current desktop wallpaper with the image located at \"$HOME/xcmd.png\""
}
]
}
